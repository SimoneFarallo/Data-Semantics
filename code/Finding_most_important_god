ef is_adjective(word: str) -> bool:\n","  return check_pos_tag(word, 'JJ')"],"metadata":{"id":"FEuGb-KqP7fn"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["def find_similar(word: str, n_synonyms: int, pos_tag_prefix: str, models: List[Word2Vec]) -> List[str]: #word to explore, number of synonyms we want, used model\n","\n","  highest_rated_nouns = dict()\n","\n","  for i in range(number_of_models):\n","    # get list of the most similar words in single model, filtered by part of speech\n","    synonyms = models[i].wv.most_similar(word, topn=(5*n_synonyms))\n","    synonyms_nouns = list(filter(lambda synonym: check_pos_tag(synonym[0], pos_tag_prefix), synonyms))\n","\n","    # combine words from different models\n","    for (noun, similarity) in synonyms_nouns:\n","      if noun not in highest_rated_nouns.keys():\n","        highest_rated_nouns[noun] = similarity\n","      else:\n","        highest_rated_nouns[noun] = max(highest_rated_nouns[noun], similarity)\n","\n","  # sort nouns by similarity (desc.) and select specified number of them\n","  highest_rated_nouns_sorted = sorted(highest_rated_nouns.items(), key=lambda item: item[1], reverse=True)[:n_synonyms]\n","  synonyms_nouns = [w for (w, sim) in highest_rated_nouns_sorted]\n","\n","  return synonyms_nouns\n","\n","\n","def find_synonyms_nouns(word: str, n_synonyms: int, models: List[Word2Vec]) -> List[str]:\n","  return find_similar(word, n_synonyms, 'NN', models)\n","\n","def find_describing_adjectives(words: List[str], n_adjectives: int, models: List[Word2Vec]) -> None:\n","  for word in words:\n","    print(find_similar(word, n_adjectives, 'JJ', models))"],"metadata":{"id":"6QhQi6tmwc76"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# for word_to_explore in words_to_explore:\n","#   for holy_text, model_list in models.items():\n","#     print(f\"{word_to_explore} - {holy_text}: {find_synonyms_nouns(word_to_explore, 10, model_list)}\")\n","#   print()\n","\n","# 